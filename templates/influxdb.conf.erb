# This file is managed by Puppet - Changes may be overwritten

# Welcome to the InfluxDB configuration file.

# The values in this file override the default values used by the system if
# a config option is not specified.

reporting-disabled = <%= @reporting_disabled %>

[meta]
  dir = "<%= @meta_dir %>"

[data]
  dir = "<%= @data_dir %>"
  wal-dir = "<%= @data_wal_dir %>"
  index-version = "<%= @data_index_version %>"
  query-log-enabled = <%= @data_query_log_enabled %>
  series-id-set-cache-size = <%= @data_series_id_set_cache_size %>

[coordinator]

[retention]

[shard-precreation]

[monitor]
   store-interval = "<%= @monitor_store_interval %>"

[http]
  auth-enabled = <%= @http_auth_enabled %>
  bind-address = "<%= @http_bind_address %>:<%= @http_bind_port %>"
  log-enabled = <%= @http_log_enabled %>
  https-enabled = <%= @http_https_enabled %>
  https-certificate = "<%= @http_https_certificate %>"
  https-private-key = "<%= @http_https_private_key %>"
  max-body-size = <%= @http_max_body_size %>
  pprof-enabled = <%= @http_pprof_enabled %>

[logging]
  level = "<%= @logging_level %>"

[subscriber]

[[graphite]]

[[collectd]]

[[opentsdb]]

[[udp]]

[continuous_queries]

[tls]
  ciphers = [
<%- Array(@tls_ciphers).each do |cipher| -%>
    "<%= cipher %>",
<% end -%>
  ]
  min-version = "<%= @tls_min_version %>"
  max-version = "<%= @tls_max_version %>"
